{
	"name": "editor",
	"version": "0.0.1",
	"private": true,
	"dependencies": {
		"@babel/standalone": "^7.17.9",
		"@monaco-editor/react": "^4.4.5",
		"@reduxjs/toolkit": "^1.8.1",
		"acorn": "^8.7.0",
		"acorn-walk": "^8.2.0",
		"classnames": "^2.3.1",
		"i18next": "^22.4.6",
		"lodash": "^4.17.21",
		"monaco-themes": "^0.4.2",
		"nanoid": "^4.0.0",
		"path-browserify": "^1.0.1",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-i18next": "^12.1.1",
		"react-redux": "^8.0.5",
		"react-router-dom": "^6.2.2",
		"react-scripts": "5.0.1",
		"sass": "^1.49.11",
		"typescript": "^5.0.4",
		"web-vitals": "^3.1.0"
	},
	"scripts": {
		"dev": "react-scripts start",
		"build": "GENERATE_SOURCEMAP=false react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"lint": "eslint src --fix",
		"check-types": "tsc",
		"format": "prettier --ignore-path .gitignore --write \"**/*.{js,jsx,ts,tsx,json}\"",
		"check-format": "yarn run prettier -- --list-different",
		"run-parallel": "npm-run-all --parallel check-types check-format lint",
		"run-sequential": "build",
		"validate": "yarn run-parallel && yarn run-sequential"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.16.3",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/acorn": "^6.0.0",
		"@types/babel__standalone": "^7.1.4",
		"@types/jest": "^29.2.4",
		"@types/lodash": "^4.14.191",
		"@types/node": "^18.11.17",
		"@types/path-browserify": "^1.0.0",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.9",
		"@types/react-redux": "^7.1.23",
		"yarn-run-all": "^3.1.1",
		"eslint-config-custom": "*",
		"tsconfig": "*"
	}
}
